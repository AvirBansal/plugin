{
  "manifest_version": 2,
  "name": "drc",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Append Test Text"
  },
  "permissions": [
    "activeTab",
    "https://mail.google.com/*"
  ]
}
